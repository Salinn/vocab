<%= form_for(@course.users.last, url: create_user_path(@course.users.last), html: { class:'form-inline'}) do |f| %>
    <% if @course.users.last.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>
          <ul>
            <% @course.users.last.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
    <% end %>
    <div class="registration_space">
      <%= f.text_field :first_name, autofocus: true, :placeholder => 'First Name', :class => 'col-md-5 form-control' %>
      <%= f.text_field :last_name,:placeholder => 'Last Name',:class => 'col-md-5 form-control' %>

      <%= f.email_field :email, :placeholder => 'Email', :class => 'col-md-3 form-control' %>
      <%= f.hidden_field :course_id, value: @course.id %>


    </div>
    </br>
    <div class="actions">
      <%= f.submit 'Submit Registration' , class:'btn btn-primary col-md-2'%>
    </div>
<% end %>