<p id="notice"><%= notice %></p>

<h1>Course List</h1>

<%= link_to 'New Course', new_course_path ,  class:"btn btn-primary"%>


<table class="table">
  <thead>
    <tr>
      <th>Class name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="courses">
    <% @courses.each do |course| %>
      <%= render 'course_row', course: course %>
    <% end %>
  </tbody>
</table>

<%=render 'layouts/dialog' %>

